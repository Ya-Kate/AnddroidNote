<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/Three"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="@null"
        android:scaleType="centerCrop"
        android:src="@drawable/bubbles03" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/skip_fragmentThree"
        android:layout_width="@dimen/dp_48"
        android:layout_height="@dimen/dp_48"
        android:layout_marginStart="@dimen/dp_314"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@null"
        android:fontFamily="@font/public_sans_medium"
        android:text="@string/skip"
        android:textAllCaps="false"
        android:textSize="@dimen/sp_18" />

    <TextView
        android:id="@+id/spiice_activityThree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_144"
        android:layout_marginTop="@dimen/dp_48"
        android:fontFamily="@font/red_hat_display_medium"
        android:padding="@dimen/dp_8"
        android:text="@string/spiice"
        android:textColor="@color/spiice"
        android:textSize="@dimen/sp_32" />

    <ImageView
        android:layout_width="315dp"
        android:layout_height="@dimen/dp_354"
        android:layout_marginStart="@dimen/dp_47"
        android:layout_marginTop="@dimen/dp_148"
        android:contentDescription="@null"
        android:src="@drawable/illustration03" />

    <TextView
        android:id="@+id/make_money"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_74"
        android:layout_marginTop="@dimen/dp550"
        android:layout_marginEnd="@dimen/dp_74"
        android:fontFamily="@font/public_sans_regular"
        android:gravity="center"
        android:maxLines="2"
        android:text="@string/make_money"
        android:textAlignment="center"
        android:textColor="@color/spiice"
        android:textSize="@dimen/sp_18" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_172"
        android:layout_marginTop="@dimen/dp650"
        android:contentDescription="@null"
        android:src="@drawable/diaporama02" />

</FrameLayout>